{
    "name": "max/max",
    "license": "Apache-2.0",
    "description": "A lightweight, high-performance PHP framework based on swoole.",
    "homepage": "https://github.com/topyao/max",
    "keywords": [
        "maxphp",
        "flexible",
        "php framework"
    ],
    "autoload": {
        "psr-4": {
            "Max\\Aop\\": "src/aop/src",
            "Max\\Cache\\": "src/cache/src",
            "Max\\Config\\": "src/config/src",
            "Max\\Context\\": "src/context/src",
            "Max\\Database\\": "src/database/src",
            "Max\\Di\\": "src/di/src",
            "Max\\Reflection\\": "src/reflection/src",
            "Max\\Event\\": "src/event/src",
            "Max\\Framework\\": "src/framework/src",
            "Max\\Http\\": "src/http/src",
            "Max\\Log\\": "src/log/src",
            "Max\\Queue\\": "src/queue/src",
            "Max\\Routing\\": "src/routing/src",
            "Max\\Swoole\\": "src/swoole/src",
            "Max\\Session\\": "src/session/src",
            "Max\\Swagger\\": "src/swagger/src",
            "Max\\Utils\\": "src/utils/src",
            "Max\\Validator\\": "src/validator/src",
            "Max\\View\\": "src/view/src",
            "Max\\Workerman\\": "src/workerman/src"
        },
        "files": [
            "src/di/src/helpers.php",
            "src/utils/src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "chengyao",
            "email": "987861463@qq.com"
        }
    ],
    "replace": {
        "max/cache": "*",
        "max/config": "*",
        "max/database": "*",
        "max/di": "*",
        "max/aop": "*",
        "max/event": "*",
        "max/framework": "*",
        "max/http": "*",
        "max/log": "*",
        "max/queue": "*",
        "max/routing": "*",
        "max/swoole": "*",
        "max/session": "*",
        "max/swagger": "*",
        "max/utils": "*",
        "max/validator": "*",
        "max/view": "*",
        "max/workerman": "*"
    },
    "require": {
        "php": "^8.0",
        "ext-swoole": ">=4.6",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "psr/container": "^2.0",
        "psr/simple-cache": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^1.0",
        "monolog/monolog": "^2.3"
    }
}
